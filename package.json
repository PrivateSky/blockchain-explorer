{
  "name": "psk-explorer",
  "version": "0.1.0",
  "description": "Explorer for PrivateSky",
  "engines": {
    "npm": ">=3"
  },
  "scripts": {
    "preinstall": "node tools/nodeVersionCheck.js",
    "start-message": "babel-node tools/startMessage.js",
    "prestart": "npm run start-message",
    "start": "concurrently -k -r -s first \"npm run open:src\" \"npm run lint:watch\" \"npm run start:api\"",
    "start:front": "concurrently -k -r -s first \"npm run open:src\" \"npm run lint:watch\"",
    "start:api": "cd api && set NODE_ENV=development && nodemon app.js --ignore Logs --ignore Sessions",
    "open:src": "babel-node tools/srcServer.js",
    "open:dist": "babel-node tools/distServer.js",
    "lint": "esw webpack.config.* src tools --color",
    "lint:watch": "npm run lint -- --watch",
    "clean-dist": "npm run remove-dist && mkdir dist",
    "remove-dist": "rimraf ./dist",
    "prebuild": "npm run clean-dist",
    "prebuild-init": "npm run lint && npm run test && npm run clean-dist",
    "build": "babel-node tools/build.js && npm run open:dist",
    "prebuild-static": "npm run clean-dist",
    "build-static": "babel-node tools/build.js",
    "test": "jest",
    "test:cover": "npm run test -- --coverage ",
    "test:watch": "jest --watch",
    "open:cover": "npm run test:cover && opn ./coverage/lcov-report/index.html",
    "analyze-bundle": "babel-node ./tools/analyzeBundle.js",
    "make-deploy-zip": "babel-node tools/make-deploy-zip.js",
    "build-deploy-zip": "npm run build-static && npm run make-deploy-zip"
  },
  "author": "space-giant",
  "license": "ISC",
  "dependencies": {
    "@date-io/moment": "1.0.1",
    "@fortawesome/fontawesome-svg-core": "1.2.12",
    "@fortawesome/free-regular-svg-icons": "5.6.3",
    "@fortawesome/free-solid-svg-icons": "5.6.3",
    "@fortawesome/react-fontawesome": "0.1.3",
    "@material-ui/core": "3.6.0",
    "@material-ui/icons": "3.0.1",
    "@material-ui/lab": "3.0.0-alpha.30",
    "bluebird": "3.5.3",
    "classnames": "2.2.6",
    "connected-react-router": "5.0.1",
    "lodash.debounce": "4.0.8",
    "material-ui-flat-pagination": "3.2.0",
    "material-ui-pickers": "2.1.1",
    "moment": "2.29.2",
    "object-assign": "4.1.1",
    "react": "16.3.2",
    "react-copy-to-clipboard": "5.0.1",
    "react-dom": "16.3.2",
    "react-dropzone": "8.0.3",
    "react-intl": "2.4.0",
    "react-json-view": "1.19.1",
    "react-jss": "8.1.0",
    "react-loading-screen": "0.0.17",
    "react-material-ui-form-validator": "2.0.2",
    "react-redux": "5.0.7",
    "react-router-dom": "4.3.1",
    "react-select": "2.3.0",
    "react-tap-event-plugin": "3.0.2",
    "react-toastify": "4.5.2",
    "recompose": "0.30.0",
    "redux": "3.6.0",
    "redux-thunk": "2.1.0",
    "whatwg-fetch": "2.0.3"
  },
  "devDependencies": {
    "archiver": "3.0.0",
    "autoprefixer": "7.1.4",
    "babel-cli": "6.24.1",
    "babel-core": "6.25.0",
    "babel-eslint": "7.2.3",
    "babel-jest": "20.0.3",
    "babel-loader": "7.1.1",
    "babel-plugin-transform-react-constant-elements": "6.23.0",
    "babel-plugin-transform-react-remove-prop-types": "0.4.6",
    "babel-polyfill": "6.23.0",
    "babel-preset-env": "1.6.0",
    "babel-preset-react": "6.24.1",
    "babel-preset-react-hmre": "1.1.1",
    "babel-preset-stage-1": "6.24.1",
    "browser-sync": "2.18.12",
    "chalk": "2.0.1",
    "concurrently": "3.5.0",
    "connect-history-api-fallback": "1.3.0",
    "coveralls": "2.13.1",
    "css-loader": "0.28.7",
    "enzyme": "3.5.0",
    "enzyme-adapter-react-16": "1.3.1",
    "enzyme-react-intl": "2.0.3",
    "eslint": "5.4.0",
    "eslint-config-prettier": "2.9.0",
    "eslint-import-resolver-webpack": "0.8.4",
    "eslint-plugin-import": "2.14.0",
    "eslint-plugin-jest": "22.1.3",
    "eslint-plugin-prettier": "2.6.0",
    "eslint-plugin-react": "7.11.1",
    "eslint-watch": "4.0.2",
    "extract-text-webpack-plugin": "3.0.1",
    "file-loader": "0.11.2",
    "history": "4.7.2",
    "html-webpack-plugin": "2.29.0",
    "http-proxy-middleware": "0.18.0",
    "identity-obj-proxy": "3.0.0",
    "jest": "24.0.0",
    "jest-cli": "20.0.4",
    "jest-fetch-mock": "2.1.0",
    "json-loader": "0.5.4",
    "mockdate": "2.0.1",
    "ncp": "2.0.0",
    "node-sass": "4.9.3",
    "opn-cli": "3.1.0",
    "postcss-loader": "2.0.6",
    "prettier": "1.14.0",
    "prompt": "1.0.0",
    "prop-types": "15.5.10",
    "raf": "3.3.2",
    "react-hot-loader": "4.6.1",
    "react-test-renderer": "16.7.0",
    "redux-immutable-state-invariant": "2.0.0",
    "redux-mock-store": "1.3.0",
    "replace": "0.3.0",
    "rimraf": "2.6.1",
    "sass-loader": "6.0.6",
    "style-loader": "0.19.0",
    "url-loader": "0.5.9",
    "webpack": "3.1.0",
    "webpack-bundle-analyzer": "2.8.2",
    "webpack-dev-middleware": "1.11.0",
    "webpack-hot-middleware": "2.18.2",
    "webpack-md5-hash": "0.0.5"
  },
  "keywords": [],
  "repository": {
    "type": "git",
    "url": "https://space-giant@bitbucket.org/space-giant/psk-explorer.git"
  },
  "jest": {
    "moduleNameMapper": {
      "\\.(jpg|jpeg|png|gif|eot|otf|webp|svg|ttf|woff|woff2|mp4|webm|wav|mp3|m4a|aac|oga)$": "<rootDir>/tools/assetsTransformer.js",
      "\\.(css)$": "<rootDir>/tools/assetsTransformer.js",
      "^core(.*)$": "<rootDir>/src/core$1",
      "^components(.*)$": "<rootDir>/src/components$1",
      "^configs(.*)$": "<rootDir>/src/configs$1",
      "^constants(.*)$": "<rootDir>/src/constants$1",
      "^locales(.*)$": "<rootDir>/src/locales$1",
      "^utils(.*)$": "<rootDir>/src/utils$1",
      "^features(.*)$": "<rootDir>/src/features$1",
      "^test(.*)$": "<rootDir>/src/test$1"
    },
    "setupFiles": [
      "raf/polyfill",
      "./tools/enzymeTestAdapterSetup.js"
    ],
    "testPathIgnorePatterns": [
      "<rootDir>/api/",
      "<rootDir>/node_modules/",
      "<rootDir>/coverage/",
      "<rootDir>/tools/"
    ],
    "collectCoverageFrom": [
      "src/**/*.js"
    ],
    "testURL": "http://localhost"
  },
  "babel": {
    "env": {
      "development": {
        "presets": [
          "env",
          "react",
          "stage-1"
        ],
        "plugins": [
          "react-hot-loader/babel"
        ]
      },
      "production": {
        "presets": [
          [
            "env",
            {
              "es2015": {
                "modules": false
              },
              "targets": {
                "ie": 9,
                "uglify": true
              }
            }
          ],
          "react",
          "stage-1"
        ],
        "plugins": [
          "transform-react-constant-elements",
          "transform-react-remove-prop-types"
        ]
      },
      "test": {
        "presets": [
          "env",
          "react",
          "stage-1"
        ]
      }
    }
  }
}
